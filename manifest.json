{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Read With Me",
  "description": "This is a Chrome extension built with React and TypeScript",
  "action": {
    "default_title": "Read With Me"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": ["tabs", "storage", "activeTab", "scripting"]
}
